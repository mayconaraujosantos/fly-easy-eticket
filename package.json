{
  "name": "eticket",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preci": "rm -rf node_modules",
    "ci": "pnpm install --frozen-lockfile",
    "build": "babel src --out-dir dist",
    "start": "node dist/index.js",
    "dev": "nodemon --exec pnpm restart",
    "debug": "rimraf dist && pnpm run build && ndb dist/index.js",
    "restart": "rimraf dist && pnpm run build && pnpm run start",
    "test": "jest --passWithNoTests",
    "test:ci": "pnpm test --coverage --silent",
    "test:unit": "pnpm test -- --watch -c jest-unit-config.js",
    "test:integration": "pnpm test -- --watch -c jest-integration-config.js",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "pnpm prettier",
      "pnpm lint:fix",
      "pnpm test",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-object-rest-spread": "^7.23.4",
    "@babel/preset-env": "^7.21.4",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.3.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "git-commit-msg-linter": "^4.10.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-puppeteer": "^6.2.0",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.8",
    "puppeteer": "^19.2.2",
    "puppeteer-core": "^21.6.1",
    "rimraf": "^4.1.1"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "winston": "^3.8.2",
    "yarn": "^1.22.19"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
