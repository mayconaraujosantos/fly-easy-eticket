{
  "name": "fly-easy-eticket-123milhas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"ts-node src\"",
    "compose": "docker-compose down --remove-orphans && docker-compose up --build --force-recreate",
    "compose:d": "npm run compose -- -d",
    "compile": "esbuild src --outfile=.tmp/dist/index.js --bundle --minify --platform=node --tree-shaking=true",
    "test": "jest --passWithNoTests --runInBand --detectOpenHandles --silent --noStackTrace",
    "test:v": "jest --passWithNoTests --runInBand --detectOpenHandles --verbose",
    "test:w": "npm run test -- --watch",
    "test:u": "npm run test -- -c jest.unit.config.ts",
    "test:i": "npm run test -- -c jest.integration.config.ts",
    "test:ci": "npm run test -- --coverage",
    "test:ci:u": "npm run test:u -- --coverage",
    "test:ci:i": "npm run test:i -- --coverage",
    "test:staged": "npm test -- --findRelatedTests",
    "lint": "eslint . --fix",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "keywords": [
    "scraper eticket"
  ],
  "author": "Maycon Santos",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@jest/types": "29.6.3",
    "@types/jest": "29.5.11",
    "@types/node": "20.8.2",
    "@types/supertest": "6.0.0",
    "commitlint": "18.4.3",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.10",
    "gts": "^5.2.0",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lefthook": "1.5.5",
    "lint-staged": "15.2.0",
    "nodemon": "3.0.2",
    "rimraf": "5.0.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "~5.2.0"
  }
}
